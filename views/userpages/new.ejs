<% include ../partials/header %>


    <div class="row">
        <h1 style="text-align: center">Create Your Page</h1>
        <div style="width: 30%; margin: 25px auto">
             <form action="/userpages" method="POST">
                 <div class="form-group">
                     <label for="name">Name</label>
                     <input class="form-control" type="text" name="name" placeholder="name">
                 </div>
                 <div class="form-group">
                     <label for="image">Image Url</label>
                     <input class="form-control" type="text" name="image" placeholder="image url">
                 </div>
                  <div class="form-group">
                     <label for="description">Description</label>
                     <input class="form-control" type="text" name="description" placeholder="describe yourself">
                 </div>
                 <div class="form-group">
                      <button class="btn btn-lg btn-default btn-block btn-primary">Submit!</button>
                 </div>
               
            </form>
            <a href="/userpages">Go Back</a>
        </div>
       
    
        
        
    </div>



<% include ../partials/footer %>